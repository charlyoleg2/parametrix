{
	"name": "designix-script-js",
	"version": "1.0.0",
	"description": "A sample code showcasing the usage of geometrix and designix",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parametrix.git"
	},
	"homepage": "https://charlyoleg2.github.io/parametrix/",
	"author": "charlyoleg",
	"license": "ISC",
	"keywords": [
		"cad",
		"programmatic",
		"2D",
		"circle",
		"stroke",
		"arc",
		"gear"
	],
	"type": "module",
	"exports": {
		".": {
			"default": "./src/index.js"
		}
	},
	"bin": "./src/index.js",
	"prettier": {
		"useTabs": true,
		"singleQuote": true,
		"trailingComma": "none",
		"printWidth": 100,
		"plugins": [],
		"overrides": []
	},
	"scripts": {
		"pretty": "prettier --check .",
		"format": "prettier --write .",
		"lint": "eslint .",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"test:bats": "bats test/test1.bats",
		"ci": "run-s pretty lint test:unit:once test:bats",
		"run": "src/index.js",
		"clean": "rimraf build dist node_modules output"
	},
	"dependencies": {
		"designix": "^1.0.28",
		"geomcli": "^1.0.28",
		"geometrix": "^1.0.28"
	},
	"devDependencies": {
		"@eslint/js": "^10.0.1",
		"bats": "^1.13.0",
		"eslint": "^10.0.0",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^17.3.0",
		"npm-run-all2": "^8.0.4",
		"prettier": "^3.8.1",
		"rimraf": "^6.1.3",
		"vitest": "^4.0.18"
	}
