{
	"name": "geomui",
	"version": "0.5.27",
	"description": "The svelte-library of the webapp-UI of Parametrix",
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parametrix.git"
	},
	"homepage": "https://charlyoleg2.github.io/parametrix/",
	"keywords": [
		"cad",
		"programmatic",
		"2D",
		"circle",
		"stroke",
		"arc",
		"gear"
	],
	"author": "charlyoleg",
	"scripts": {
		"cp_pgdsvg": "shx cp -r ../../node_modules/designix/dist/pgdsvg static/",
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"ci": "run-s cp_pgdsvg build lint check test:unit:once",
		"format": "prettier --write .",
		"clean": "shx rm -fr .svelte-kit build node_modules dist static/pgdsvg"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.0.3",
		"svelte": "^4.0.0"
	},
	"dependencies": {
		"geometrix": "^0.5.24"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.4",
		"@sveltejs/package": "^2.3.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"designix": "^0.5.25",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"publint": "^0.2.7",
		"sass": "^1.72.0",
		"shx": "^0.3.4",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.8",
		"tslib": "^2.6.2",
		"typescript": "^5.4.3",
		"vite": "^5.2.2",
		"vitest": "^1.4.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.map",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
