{
	"name": "designix",
	"version": "0.5.13",
	"description": "The first designs made with parametrix",
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parametrix.git"
	},
	"homepage": "https://github.com/charlyoleg2/parametrix/tree/main/pkg/designix#readme",
	"keywords": [
		"cad",
		"programmatic",
		"2D",
		"circle",
		"stroke",
		"arc",
		"gear"
	],
	"author": "charlyoleg",
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"format": "esm",
		"splitting": false,
		"dts": true,
		"sourcemap": true,
		"clean": true
	},
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"check": "tsc --noEmit",
		"pretty": "prettier --check .",
		"format": "prettier --write .",
		"lint": "eslint .",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"ci": "run-s check build pretty lint test:unit:once",
		"svgo1": "svgo --config ../paramui/svgo.config.cjs -f src/svg -o src/svgo",
		"svgo2": "svgo --config ../paramui/svgo.config.cjs -f src/dummy/svg -o src/dummy/svgo",
		"svgo3": "svgo --config ../paramui/svgo.config.cjs -f src/gears/svg -o src/gears/svgo",
		"svgo4": "svgo --config ../paramui/svgo.config.cjs -f src/geometrix_verification/svg -o src/geometrix_verification/svgo",
		"svgo5": "svgo --config ../paramui/svgo.config.cjs -f src/heliostat/svg -o src/heliostat/svgo",
		"svgoAll": "run-s svgo1 svgo2 svgo3 svgo4 svgo5",
		"svg2ts1": "svgfiles2js -s src/svgo/*.svg -o src/svg_bundle.js",
		"svg2ts2": "svgfiles2js -s src/dummy/svgo/*.svg -o src/dummy/svg_dummy.js",
		"svg2ts3": "svgfiles2js -s src/gears/svgo/*.svg -o src/gears/svg_gears.js",
		"svg2ts4": "svgfiles2js -s src/geometrix_verification/svgo/*.svg -o src/geometrix_verification/svg_geomverif.js",
		"svg2ts5": "svgfiles2js -s src/heliostat/svgo/*.svg -o src/heliostat/svg_heliostat.js",
		"svg2tsAll": "run-s svg2ts1 svg2ts2 svg2ts3 svg2ts4 svg2ts5",
		"svgCp1": "shx cp export_svg/*.svg ../geomui/static",
		"svgCp2": "shx cp export_svg/*.svg ../designix-ui/static",
		"svgCp3": "shx cp export_svg/*.svg ../paramui/static",
		"svgo": "run-s svgoAll svg2tsAll",
		"svginline": "svgfiles2js --help",
		"clean": "shx rm -fr dist node_modules"
	},
	"dependencies": {
		"geometrix": "^0.5.12"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.18.0",
		"@typescript-eslint/parser": "^6.18.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.1.1",
		"shx": "^0.3.4",
		"svgfiles2js": "^0.1.8",
		"svgo": "^3.2.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "^1.1.3"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.map",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"types": "./dist/index.d.ts",
	"type": "module"
}
