{
	"name": "designix",
	"version": "0.1.1",
	"description": "The first designs made with parametrix",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parametrix.git"
	},
	"homepage": "https://github.com/charlyoleg2/parametrix#readme",
	"keywords": [
		"cad",
		"programmatic",
		"2D",
		"circle",
		"stroke",
		"arc",
		"gear"
	],
	"author": "charlyoleg",
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"format": "esm",
		"splitting": false,
		"dts": true,
		"sourcemap": true,
		"clean": true
	},
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"check": "tsc --noEmit",
		"pretty": "prettier --check .",
		"format": "prettier --write .",
		"lint": "eslint .",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"ci": "run-s check build pretty lint test:unit:once",
		"svgo1": "svgo -f src/svg -o ../paramui/static",
		"svgo2": "svgo -f src/dummies/svg -o ../paramui/static",
		"svgo3": "svgo -f src/gears/svg -o ../paramui/static",
		"svgo4": "svgo -f src/geometrix_verification/svg -o ../paramui/static",
		"svgo5": "svgo -f src/heliostat/svg -o ../paramui/static",
		"svgo": "run-s svgo1 svgo2 svgo3 svgo4 svgo5",
		"clean": "shx rm -fr dist node_modules"
	},
	"dependencies": {
		"geometrix": "^1.0.1"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.3",
		"shx": "^0.3.4",
		"svgo": "^3.0.2",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	},
	"exports": {
		".": "dist/index.js"
	},
	"types": "dist/index.d.ts",
	"type": "module"
}
