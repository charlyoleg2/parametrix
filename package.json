{
	"name": "parametrix",
	"version": "0.5.0",
	"description": "The root directory of the parametrix packages",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parametrix.git"
	},
	"keywords": [
		"cad",
		"programmatic",
		"2D",
		"circle",
		"stroke",
		"arc",
		"gear"
	],
	"author": "charlyoleg2",
	"scripts": {
		"geometrix:dev": "npm -w geometrix run dev",
		"geometrix:ci": "npm -w geometrix run ci",
		"geometrix:svgo": "npm -w geometrix run svgo",
		"geometrix:clean": "npm -w geometrix run clean",
		"designix:dev": "npm -w designix run dev",
		"designix:ci": "npm -w designix run ci",
		"designix:svgo": "npm -w designix run svgo",
		"designix:clean": "npm -w designix run clean",
		"paramuilib:dev": "npm -w paramuilib run dev",
		"paramuilib:ci": "npm -w paramuilib run ci",
		"paramuilib:clean": "npm -w paramuilib run clean",
		"paramui:dev": "npm -w paramui run dev",
		"paramui:ci": "npm -w paramui run ci",
		"paramui:svgo": "npm -w paramui run svgo",
		"paramui:clean": "npm -w paramui run clean",
		"paramui:preview": "npm -w paramui run preview",
		"demo-paramuilib:ci": "npm -w demo-paramuilib run ci",
		"demo-paramuilib:clean": "npm -w demo-paramuilib run clean",
		"dev": "run-p geometrix:dev designix:dev paramuilib:dev paramui:dev",
		"ci": "run-s geometrix:ci designix:ci paramuilib:ci paramui:ci demo-paramuilib:ci",
		"ci2": "run-s designix:ci paramui:ci",
		"svgo": "run-s geometrix:svgo designix:svgo paramui:svgo",
		"clean": "run-s geometrix:clean designix:clean paramuilib:clean paramui:clean demo-paramuilib:clean",
		"preview": "run-s paramui:preview",
		"ls-pkg": "ls -ltra pkg/geometrix pkg/designix pkg/paramuilib pkg/paramui"
	},
	"workspaces": [
		"pkg/*"
	],
	"type": "module"
}
